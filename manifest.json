{
"manifest_version": 2,
    "name": "[Antimat]",
    "description": "[Delete bad words]",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "*.txt"
    ]
}